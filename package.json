{
  "name": "clinica",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "copy:public": "powershell -Command \"Get-ChildItem -Path 'public' -Exclude 'assets' | Remove-Item -Recurse -Force; Get-ChildItem -Path 'dist/clinica/browser' | Copy-Item -Destination 'public/' -Recurse -Force\"",
    "deploy": "npm run build:prod && npm run copy:public && firebase deploy --only hosting",
    "deploy:preview": "npm run build:prod && npm run copy:public && firebase hosting:channel:deploy preview --expires 7d"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/build": "^20.3.11",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@supabase/supabase-js": "^2.50.2",
    "@tailwindcss/postcss": "^4.1.8",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "postcss": "^8.5.4",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.3.0",
    "vite": "^7.2.4",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "typescript": "~5.8.2"
  }
}
